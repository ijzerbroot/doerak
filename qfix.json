{
    "title": "QFix",
    "rules": [
        {
            "description": "Qwerty quick-fix using alternate hand placement and two swaps",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "r",
                        "modifiers": {
                            "optional": [
                                "caps_lock",
                                "left_command",
                                "left_control",
                                "left_alt",
                                "left_shift",
                                "right_shift",
                                "right_command",
                                "right_control",
                                "right_alt"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "p"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "p",
                        "modifiers": {
                            "optional": [
                                "caps_lock",
                                "left_command",
                                "left_control",
                                "left_alt",
                                "left_shift",
                                "right_shift",
                                "right_command",
                                "right_control",
                                "right_alt"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "r"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "j",
                        "modifiers": {
                            "optional": [
                                "caps_lock",
                                "left_command",
                                "left_control",
                                "left_alt",
                                "left_shift",
                                "right_shift",
                                "right_command",
                                "right_control",
                                "right_alt"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "n"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "n",
                        "modifiers": {
                            "optional": [
                                "caps_lock",
                                "left_command",
                                "left_control",
                                "left_alt",
                                "left_shift",
                                "right_shift",
                                "right_command",
                                "right_control",
                                "right_alt"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "j"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "slash",
                        "modifiers": {
                            "optional": [
                                "caps_lock",
                                "left_command",
                                "left_control",
                                "left_alt",
                                "left_shift",
                                "right_shift",
                                "right_command",
                                "right_control",
                                "right_alt"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "slash"
                        }
                    ]
                }
            ]
        }
    ]
}
